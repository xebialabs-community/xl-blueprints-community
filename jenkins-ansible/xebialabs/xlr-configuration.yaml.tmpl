apiVersion: xl-release/v1
kind: Templates
spec:
- name: XLD Server
  type: xldeploy.XLDeployServer
  url: http://xld:4516
  username: !value XL_DEPLOY_USERNAME
  password: !value XL_DEPLOY_PASSWORD

- name: Ansible Server
  type: remoteScript.OverthereHost
  address: {{.ANSIBLE_HOSTNAME}}
  port: {{.ANSIBLE_PORT}}
  username: !value ANSIBLE_USERNAME
  password: !value ANSIBLE_PASSWORD

- name: Jenkins Server
  type: jenkins.Server
  url: {{.JENKINS_URL}}
  username: !value JENKINS_USERNAME
  password: !value JENKINS_PASSWORD

